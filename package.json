{
  "name": "xcodebuild-action",
  "version": "1.4.0",
  "description": "A GitHub action that runs xcodebuild.",
  "main": "dist/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
